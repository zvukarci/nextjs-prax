{
  "name": "app",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev --turbopack",
    "build": "next build --turbopack",
    "start": "next start",
    "lint": "eslint",
    "db:migrate:make": "kysely migrate:make new_migration",
    "db:migrate:latest": "kysely migrate:latest",
    "db:seed:make": "kysely seed:make new_seed",
    "db:seed:run": "kysely seed:run",
    "db:codegen": "kysely-codegen --url db.sqlite --out-file src/lib/db-types.ts"
  },
  "dependencies": {
    "better-sqlite3": "^12.2.0",
    "kysely": "^0.28.5",
    "next": "15.5.2",
    "react": "19.1.0",
    "react-dom": "19.1.0"
  },
  "devDependencies": {
    "@eslint/eslintrc": "^3",
    "@faker-js/faker": "^10.0.0",
    "@tailwindcss/postcss": "^4",
    "@types/better-sqlite3": "^7.6.13",
    "@types/node": "^20",
    "@types/react": "^19",
    "@types/react-dom": "^19",
    "daisyui": "^5.1.4",
    "eslint": "^9",
    "eslint-config-next": "15.5.2",
    "kysely-codegen": "^0.19.0",
    "kysely-ctl": "^0.19.0",
    "prettier": "^3.6.2",
    "tailwindcss": "^4",
    "tsx": "^4.20.5",
    "typescript": "^5"
  },
  "overrides": {
    "kysely-codegen": {
      "better-sqlite3": "^12.2.0"
    }
  }
}
